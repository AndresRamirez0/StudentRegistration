# EditorConfig para UTF-8
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.cs]
charset = utf-8
indent_style = space
indent_size = 4

[*.json]
charset = utf-8
indent_style = space
indent_size = 2

[*.{yml,yaml}]
charset = utf-8
indent_style = space
indent_size = 2